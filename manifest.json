{
  "name": "LetterBrush (GCAA)",
  "description": "The LetterBrush ASCII level editor ported to a Chrome App Accelerated",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/main.js",
        "js/letterbrush.js",
        "js/lib/inherit.js",
        "js/lib/jquery.js",
        "js/lib/jquery.mousewheel.min.js",
        "js/lib/keyDecode.js",
        "js/lib/jqui/jquery-ui-1.8.16.custom.min.js",
        "js/chrome_helpers.js"
      ]
    }
  },
  "permissions": [
    {"fileSystem": ["write"]},
    "storage"
  ]
}